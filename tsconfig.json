{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2016",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": false,
    "sourceMap": true,
    "outDir": "dist"
  },
  "lib": ["es2015"]
}
